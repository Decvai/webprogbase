<div class="wrapper">
	<div class="new-university">
		<div class="new-university__title title"><img id="main-logo" src="/img/icons/logo.png" alt="logo" /> Многостраничный веб сайт</div>
		<div class="new-university__row row">
			<div class="new-university__menu menu">
				{{>menu}}
			</div>
			<div class="new-university__content content">
				<form method="post" enctype="multipart/form-data">
					<input type="text" name="name" required placeholder="Название">
					<input type="text" name="country" required placeholder="Страна">
					<input type="number" name="numOfStudents" placeholder="Количество студентов">
					<input type="number" name="campus" placeholder="Площадь">
					Дата основания: <input type="date" name="foundationDate">
					Изображение университета: <input type="file" name="unImage">
					<input class="submit" type="submit" value="Создать">
				</form>
			</div>
		</div>
	</div>
</div>